{
  "name": "kudoc-cameroon",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "nextjs-kudoc"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=nextjs-kudoc",
    "build": "npm run build --workspace=nextjs-kudoc",
    "start": "npm run start --workspace=nextjs-kudoc",
    "serve-static": "npx serve .",
    "dev:all": "concurrently \"npm run serve-static\" \"npm run dev\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "serve": "^14.2.1"
  }
}